{
    "name": "akatsuki/torrent",
    "type": "library",
    "description": "Akatsuki Torrent Component",
    "keywords": ["torrent", "akatsuki"],
    "homepage": "https://akatsuki.se",
    "license": "MIT",
    "authors": [
        {
            "name": "Aurimas Niekis",
            "email": "aurimas@niekis.lt"
        }
    ],
    "require": {
        "php": "^7.1",
        "akatsuki/bencode": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.1"
    },
    "autoload": {
        "psr-4": { "Akatsuki\\Component\\Torrent\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "Akatsuki\\Component\\Torrent\\Test\\": "tests" }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-ci": "vendor/bin/phpunit -c phpunit.xml.ci"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
